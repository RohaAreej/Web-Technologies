  <section class="menu" id="menu">

    <h1 class="heading">menu</h1>
  
    <a href="/add" class="btn" style="align-items: center; margin:20px">Add menu</a>

    <div class="box-container">
  
      <% menu.forEach(function(item) { %>
        <div class="box">
            <img src="<%= item.image %>" alt="">
            <h3><%= item.name %></h3>
            <div class="price"><%= item.price %></div>
            <!-- <button class="btn add-to-cart">Add to Cart</button>
            <button class="btn edit">Edit</button>
            <button class="btn delete">Delete</button> -->
            <a href="/cart/<%= item._id %>" class="btn">Add to Cart</a>
            <a href="/edit/<%= item._id %>" class="btn">Edit</a>
            <a href="/delete/<%= item._id %>" class="btn">Delete</a>
        </div>
      <% }); %>
  
    </div>
  
  </section>
  

<!-- end -->

<!-- Contact US -->

<section class="contact" id="contact">

    <h1 class="heading">Contact Us</h1>

    <div class="row">

        <div class="image">
            <img src="images/contact.png" alt="">
        </div>

        <form action="/contact" method="post">
            <h3>Get in touch</h3>

            <div class="inputBox">
                <span class="fas fa-user"></span>
                <input name="name" type="text" placeholder="Name">
            </div>

            <div class="inputBox">
                <span class="fas fa-envelope"></span>
                <input name="email" type="email" placeholder="Email">
            </div>

            <div class="inputBox">
                <span class="fas fa-phone"></span>
                <input name="phone" type="number" placeholder="Phone">
            </div>

            <div class="inputBox">
                <span class="fas fa-comment"></span>
                <textarea name="comment" placeholder="Write something" style="border: none; padding: 10px;width: 100%; height: 100%; background-color: transparent; color: white;"></textarea>
            </div>
            
            
            <div>
            <input type="submit" value="Send Message" class="btn">
            </div>
        </form>

    </div>

</section>


